{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["ES2022"],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "moduleDetection": "force",
    "typeRoots": ["./types", "./node_modules/@types/"],
    "skipLibCheck": true
  }
}
